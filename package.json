{
  "name": "rune-decorator",
  "version": "0.0.6",
  "description": "rune-ts decorators util",
  "scripts": {
    "build:esm": "tsc && tsc-esm-fix",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs",
    "test": "jest",
    "prepack": "npm run build"
  },
  "author": "choie0423@naver.com",
  "license": "ISC",
  "exports": {
    "./client": {
      "import": "./dist/esm/src/client/index.js",
      "require": "./dist/cjs/src/client/index.js",
      "types": "./dist/types/src/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/esm/src/server/index.js",
      "require": "./dist/cjs/src/server/index.js",
      "types": "./dist/types/src/server/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/easdkr/rune-decorator.git"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/express-serve-static-core": "^5.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsc-esm-fix": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@fxts/core": "^1.4.0",
    "@rune-ts/server": "^1.0.23",
    "express": "^4.21.2",
    "reflect-metadata": "^0.2.2",
    "rune-ts": "^0.8.12",
    "ts-loader": "^9.5.1",
    "typedi": "^0.10.0"
  },
  "peerDependencies": {
    "rune-ts": "^0.8.12"
  }
}
